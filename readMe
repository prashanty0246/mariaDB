 const INSERT_ISSUANCE_REQ =
      "INSERT INTO token_request (id,status,sender_organization,receiver_organization,sender_address,receiver_address,tokens_requested,total_amount,maker_name, maker_remarks,checker_name, checker_remarks,checked_at, doc_type,created_at,updated_at,txn_ref_no, sub_type, req_msg_id, recall_id) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?)";
    const result = await tokenRequestRepository.query(INSERT_ISSUANCE_REQ, [
      request.id,
      requestDbRecord.status,
      requestDbRecord.sender_organization,
      requestDbRecord.receiver_organization,
      requestDbRecord.sender_address,
      requestDbRecord.receiver_address,
      requestDbRecord.tokens_requested,
      JSON.parse(requestDbRecord.total_amount),
      requestDbRecord.maker_name,
      requestDbRecord.maker_remarks,
      requestDbRecord.checker_name,
      requestDbRecord.checker_remarks,
      requestDbRecord.checked_at,
      requestDbRecord.doc_type,
      requestDbRecord.created_at,
      requestDbRecord.updated_at,
      requestDbRecord.txn_ref_no,
      requestDbRecord.sub_type,
      requestDbRecord.req_msg_id,
      requestDbRecord.recall_id,
    ]);
